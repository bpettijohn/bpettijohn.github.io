#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/bpettijohn/dev/ben/bpettijohn.github.io/node_modules/esbuild/bin/node_modules:/Users/bpettijohn/dev/ben/bpettijohn.github.io/node_modules/esbuild/node_modules:/Users/bpettijohn/dev/ben/bpettijohn.github.io/node_modules:/Users/bpettijohn/dev/ben/node_modules:/Users/bpettijohn/dev/node_modules:/Users/bpettijohn/node_modules:/Users/node_modules:/node_modules:/Users/bpettijohn/dev/ben/bpettijohn.github.io/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/bpettijohn/dev/ben/bpettijohn.github.io/node_modules/esbuild/bin/node_modules:/Users/bpettijohn/dev/ben/bpettijohn.github.io/node_modules/esbuild/node_modules:/Users/bpettijohn/dev/ben/bpettijohn.github.io/node_modules:/Users/bpettijohn/dev/ben/node_modules:/Users/bpettijohn/dev/node_modules:/Users/bpettijohn/node_modules:/Users/node_modules:/node_modules:/Users/bpettijohn/dev/ben/bpettijohn.github.io/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
"$basedir/../esbuild/bin/esbuild"   "$@"
exit $?
